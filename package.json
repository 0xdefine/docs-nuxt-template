{
  "name": "@matterlabs/zksync-docs-nuxt-template",
  "type": "module",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "files": [
    "cspell-config",
    "assets",
    "components",
    "composables",
    "lib",
    "modules",
    "plugins",
    "public",
    "types",
    "app.config.ts",
    "nuxt.config.ts",
    "tailwind.config.ts"
  ],
  "scripts": {
    "postinstall": "nuxt prepare",
    "dev": "nuxi dev .playground",
    "build": "nuxt generate .playground",
    "lint": "eslint .",
    "prepare": "nuxt prepare .playground",
    "lint:spelling": "cspell **/*.md --config=./cspell.json",
    "lint:markdown": "markdownlint-cli2 \"content/**/*.md\" --config \".markdownlint.json\"",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "ci:check": "bun run lint:eslint && bun run lint:prettier && bun run lint:spelling && bun run lint:markdown"
  },
  "devDependencies": {
    "cspell": "^8.8.0",
    "nuxt": "^3.6.2"
  },
  "dependencies": {
    "@iconify-json/devicon": "^1.1.41",
    "@iconify-json/heroicons": "^1.1.21",
    "@iconify-json/simple-icons": "^1.1.101",
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/fonts": "^0.7.0",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.16.0",
    "@nuxt/ui-pro": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.25.0",
    "markdownlint": "^0.34.0",
    "markdownlint-cli2": "^0.13.0",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-og-image": "^3.0.0-rc.53",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0",
    "typescript": "^4.9.3",
    "vue-easy-lightbox": "^1.19.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.16"
  }
}
